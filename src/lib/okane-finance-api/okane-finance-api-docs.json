		"/news/": {
			"get": {
				"tags": ["news"],
				"summary": "Archive Ticker News",
				"operationId": "archive_ticker_news_news__get",
				"parameters": [
					{
						"name": "from_date",
						"in": "query",
						"required": true,
						"schema": { "type": "string", "title": "From Date" }
					},
					{
						"name": "to_date",
						"in": "query",
						"required": true,
						"schema": { "type": "string", "title": "To Date" }
					},
					{
						"name": "tickers",
						"in": "query",
						"required": false,
						"schema": { "anyOf": [{ "type": "string" }, { "type": "null" }], "title": "Tickers" }
					},
					{
						"name": "limit",
						"in": "query",
						"required": false,
						"schema": {
							"anyOf": [{ "type": "integer" }, { "type": "null" }],
							"default": 1000,
							"title": "Limit"
						}
					},
					{
						"name": "sort",
						"in": "query",
						"required": false,
						"schema": {
							"anyOf": [{ "type": "string" }, { "type": "null" }],
							"default": "RELEVANCE",
							"title": "Sort"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Successful Response",
						"content": {
							"application/json": {
								"schema": { "$ref": "#/components/schemas/AlphaVantageNewsResponseDTO" }
							}
						}
					},
					"404": { "description": "Not found" },
					"422": {
						"description": "Validation Error",
						"content": {
							"application/json": {
								"schema": { "$ref": "#/components/schemas/HTTPValidationError" }
							}
						}
					}
				}
			}
		},
